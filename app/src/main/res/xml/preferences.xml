<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <ListPreference
        android:defaultValue="0"
        android:entries="@array/pref_num_concurrency"
        android:entryValues="@array/pref_num_concurrency_values"
        android:key="concurrency_list"
        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:title="Concurrency Setting" />
    <ListPreference
        android:defaultValue="5"
        android:entries="@array/pref_time_alarm"
        android:entryValues="@array/pref_time_alarm_values"
        android:key="alarm_timer_list"
        android:negativeButtonText="@null"
        android:positiveButtonText="@null"
        android:title="Alarm Timer Setting" />
</PreferenceScreen>